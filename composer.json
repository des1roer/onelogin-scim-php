{
  "name": "onelogin/scim-php",
  "description": "A simple PHP API built using Slim that handles and responds to all of the SCIM requests made by OneLogin for user provisioning.",
  "require": {
    "php": ">=5.6",
    "slim/slim": "^3.12",
    "illuminate/database": "~5.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "tuupola/slim-jwt-auth": "^2.4",
    "ramsey/uuid": "~3.8"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Sixto Martin",
      "email": "sixto.garcia@onelogin.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "src",
      "App\\Controllers\\": "src/Controllers"
    }
  }
}
